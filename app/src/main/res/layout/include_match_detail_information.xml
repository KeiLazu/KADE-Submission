                android:layout_width="@dimen/zero"
                android:layout_height="wrap_content"
                android:id="@+id/include_match_detail_info_home_goalkeepers"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/include_match_detail_info_goalkeepers_separator"
                style="@style/TextAppearanceInfo.Home"
        />

        <TextView
                android:id="@+id/include_match_detail_info_goalkeepers_separator"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_goalkeeper"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/TextAppearanceInfo"
        />

        <TextView
                android:layout_width="@dimen/zero"
                android:layout_height="wrap_content"
                android:id="@+id/include_match_detail_info_away_goalkeepers"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/include_match_detail_info_goalkeepers_separator"
                android:text=""
                style="@style/TextAppearanceInfo.Away"
        />

    </android.support.constraint.ConstraintLayout>

    <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/separator_height"
            android:background="@drawable/bg_separator"
            app:layout_constraintTop_toBottomOf="@id/include_match_detail_info_goalkeepers_layout"
    />

    <android.support.constraint.ConstraintLayout

                android:layout_width="@dimen/zero"
                android:layout_height="wrap_content"
                android:id="@+id/include_match_detail_info_home_defense"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/include_match_detail_info_defense_separator"
                style="@style/TextAppearanceInfo.Home"
        />

        <TextView
                android:id="@+id/include_match_detail_info_defense_separator"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_defense"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"

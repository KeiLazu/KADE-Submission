                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/include_match_detail_info_forward_separator"
                android:text=""
                style="@style/TextAppearanceInfo.Away"
        />

    </android.support.constraint.ConstraintLayout>

    <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/separator_height"
            android:background="@drawable/bg_separator"
            app:layout_constraintTop_toBottomOf="@id/include_match_detail_info_forward_layout"
    />

    <android.support.constraint.ConstraintLayout
            android:id="@+id/include_match_detail_info_subs_layout"
            android:layout_width="@dimen/zero"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/include_match_detail_info_forward_layout"
